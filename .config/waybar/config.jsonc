{
  "modules-left": [
    "pulseaudio",
    "network",
    "cpu",
    "memory",
    "temperature",
    "backlight",
    "hyprland/language",
    "battery",
    "battery#bat2",
  ],
  "modules-center": ["hyprland/workspaces"],
  "modules-right": [
    "clock",
    "tray",
  ],
  "hyprland/window": {
    "max-length": 50,
  },
  "battery": {
    "format": "{capacity}% {icon}",
    "format-icons": [" ", " ", " ", " ", " "],
  },
  "clock": {
    "format": "{:%a, %d. %b  %H:%M}",
  },
}
