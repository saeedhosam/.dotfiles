{
  "layer": "top",
  "position": "top",
  "height": 10,
  "modules-left": ["hyprland/workspaces"],
  "modules-center": [],
  "modules-right": ["pulseaudio", "battery", "clock"],
  "hyprland/workspaces": {
    "on-click": "activate"
  },
  "pulseaudio": {
    "format": " {volume}%",
    "format-muted": " muted"
  },
  "battery": {
    "format": "{capacity}%  ",
    "format-charging": " {capacity}%"
  },
  "clock": {
    "format": " %H:%M"
  }
}

